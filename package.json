{
  "name": "@maverick-js/elements",
  "description": "",
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "module": "dist/runtime/prod/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "development": "./dist/runtime/dev/index.js",
      "test": "./dist/runtime/dev/index.js",
      "default": "./dist/runtime/prod/index.js"
    },
    "./dom": {
      "development": "./dist/runtime/dev/dom.js",
      "test": "./dist/runtime/dev/dom.js",
      "default": "./dist/runtime/prod/dom.js"
    },
    "./ssr": {
      "development": "./dist/runtime/dev/ssr.js",
      "test": "./dist/runtime/dev/ssr.js",
      "default": "./dist/runtime/prod/ssr.js"
    },
    "./jsx-runtime": "./jsx-runtime.d.ts",
    "./esbuild": "./dist/plugins/esbuild.js",
    "./rollup": "./dist/plugins/rollup.js",
    "./vite": "./dist/plugins/vite.js",
    "./webpack": "./dist/plugins/webpack.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "vite dev sandbox",
    "build": "vite build sandbox",
    "preview": "vite preview sandbox",
    "bundle": "rimraf dist && node .scripts/bundle.js",
    "test": "vitest --run",
    "format": "prettier src --write --loglevel warn",
    "test:watch": "vitest --watch",
    "validate": "pnpm test && pnpm bundle && pnpm types",
    "release": "pnpm validate && standard-version && git push --follow-tags origin main && npm publish",
    "size": "pnpm bundle && export-size .",
    "types": "rimraf [^j]*.d.ts && tsc -p .config/tsconfig.build.json && run-s types:* && rimraf dist/*.d.ts",
    "types:index": "api-extractor run -c .config/types.index.json",
    "types:dom": "api-extractor run -c .config/types.dom.json",
    "types:ssr": "api-extractor run -c .config/types.ssr.json",
    "types:esbuild": "api-extractor run -c .config/types.esbuild.json",
    "types:rollup": "api-extractor run -c .config/types.rollup.json",
    "types:vite": "api-extractor run -c .config/types.vite.json",
    "types:webpack": "api-extractor run -c .config/types.webpack.json"
  },
  "dependencies": {
    "@dprint/formatter": "^0.2.0",
    "@dprint/typescript": "^0.71.1",
    "devalue": "^2.0.1",
    "html-entities": "^2.3.3",
    "magic-string": "^0.26.2",
    "normalize-path": "^3.0.0",
    "unplugin": "^0.7.2"
  },
  "devDependencies": {
    "@maverick-js/observables": "^4.0.2",
    "@maverick-js/scheduler": "^1.0.0",
    "@microsoft/api-extractor": "^7.28.6",
    "@rollup/pluginutils": "^4.2.1",
    "@types/node": "^18.0.3",
    "esbuild": "^0.14.49",
    "export-size": "^0.5.2",
    "globby": "^13.1.2",
    "jsdom": "^20.0.0",
    "kleur": "^4.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "standard-version": "^9.5.0",
    "type-fest": "^2.17.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vitest": "^0.19.0",
    "webpack": "^5.74.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx --no-install lint-staged",
    "commit-msg": "npx --no -- commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md,json}": "prettier --write"
  },
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maverick-js/elements.git"
  },
  "bugs": {
    "url": "https://github.com/maverick-js/elements/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "volta": {
    "node": "16.15.1"
  },
  "packageManager": "pnpm@7.2.1"
}
