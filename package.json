{
  "name": "maverick.js",
  "description": "",
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "module": "dist/runtime/prod/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "development": "./dist/dev/runtime.js",
      "test": "./dist/dev/runtime.js",
      "require": "./dist/server/runtime.cjs",
      "deno": "./dist/server/runtime.js",
      "node": "./dist/server/runtime.js",
      "default": "./dist/prod/runtime.js"
    },
    "./dom": {
      "types": "./dom.d.ts",
      "development": "./dist/dev/dom.js",
      "test": "./dist/dev/dom.js",
      "require": "./dist/server/dom.cjs",
      "deno": "./dist/server/dom.js",
      "node": "./dist/server/dom.js",
      "default": "./dist/prod/dom.js"
    },
    "./ssr": {
      "types": "./ssr.d.ts",
      "development": "./dist/dev/ssr.js",
      "test": "./dist/dev/ssr.js",
      "require": "./dist/server/ssr.cjs",
      "deno": "./dist/server/ssr.js",
      "node": "./dist/server/ssr.js",
      "default": "./dist/prod/ssr.js"
    },
    "./element": {
      "types": "./element.d.ts",
      "development": "./dist/dev/element.js",
      "test": "./dist/dev/element.js",
      "require": "./dist/server/element.cjs",
      "deno": "./dist/server/element.js",
      "node": "./dist/server/element.js",
      "default": "./dist/prod/element.js"
    },
    "./plugins": {
      "types": "./plugins.d.ts",
      "require": "./dist/server/plugins.cjs",
      "default": "./dist/server/plugins.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "pnpm run build --watch",
    "build": "rimraf dist && tsup",
    "format": "prettier src --write --loglevel warn",
    "test:client": "vitest --run",
    "test:client:watch": "vitest --watch",
    "test:server": "SERVER=true vitest --run",
    "test:server:watch": "SERVER=true vitest --watch",
    "sandbox:dev": "vite dev sandbox",
    "sandbox:build": "vite build sandbox",
    "sandbox:preview": "vite preview sandbox",
    "size": "pnpm bundle && export-size .",
    "release": "pnpm test:client && pnpm test:server && pnpm build && standard-version && git push --follow-tags origin main && npm publish"
  },
  "dependencies": {
    "@dprint/formatter": "^0.2.0",
    "@dprint/typescript": "^0.71.1",
    "html-entities": "^2.3.3",
    "magic-string": "^0.26.2",
    "normalize-path": "^3.0.0",
    "type-fest": "^2.17.0",
    "unplugin": "^0.7.2"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^2.0.0",
    "@maverick-js/observables": "^4.6.0",
    "@maverick-js/scheduler": "^2.1.0",
    "@microsoft/api-extractor": "^7.28.6",
    "@rollup/pluginutils": "^4.2.1",
    "@types/node": "^18.0.3",
    "benchmark": "^2.1.4",
    "esbuild": "^0.14.49",
    "export-size": "^0.5.2",
    "globby": "^13.1.2",
    "jsdom": "^20.0.0",
    "kleur": "^4.1.5",
    "node-fetch": "^3.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "standard-version": "^9.5.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.0",
    "vite": "^3.0.3",
    "vitest": "^0.24.3",
    "webpack": "^5.74.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx --no-install lint-staged",
    "commit-msg": "npx --no -- commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md,json}": "prettier --write"
  },
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maverick-js/maverick.git"
  },
  "bugs": {
    "url": "https://github.com/maverick-js/maverick/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "volta": {
    "node": "16.15.1"
  },
  "packageManager": "pnpm@7.2.1"
}
