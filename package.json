{
  "name": "maverick-js",
  "description": "",
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "module": "dist/runtime/prod/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "development": "./dist/dev/runtime.js",
      "test": "./dist/dev/runtime.js",
      "node": "./dist/node/runtime.js",
      "default": "./dist/prod/runtime.js"
    },
    "./dom": {
      "types": "./dom.d.ts",
      "development": "./dist/dev/dom.js",
      "test": "./dist/dev/dom.js",
      "node": "./dist/node/dom.js",
      "default": "./dist/prod/dom.js"
    },
    "./ssr": {
      "types": "./ssr.d.ts",
      "development": "./dist/dev/ssr.js",
      "test": "./dist/dev/ssr.js",
      "node": "./dist/node/ssr.js",
      "default": "./dist/prod/ssr.js"
    },
    "./plugins": {
      "types": "./plugins.d.ts",
      "default": "./dist/node/plugins.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "pnpm run build --watch",
    "build": "rimraf dist && tsup",
    "format": "prettier src --write --loglevel warn",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "sandbox:dev": "vite dev sandbox",
    "sandbox:build": "vite build sandbox",
    "sandbox:preview": "vite preview sandbox",
    "size": "pnpm bundle && export-size .",
    "release": "pnpm test && pnpm build && standard-version && git push --follow-tags origin main && npm publish"
  },
  "dependencies": {
    "@dprint/formatter": "^0.2.0",
    "@dprint/typescript": "^0.71.1",
    "devalue": "^2.0.1",
    "html-entities": "^2.3.3",
    "magic-string": "^0.26.2",
    "normalize-path": "^3.0.0",
    "type-fest": "^2.17.0",
    "unplugin": "^0.7.2"
  },
  "devDependencies": {
    "@maverick-js/observables": "^4.3.2",
    "@maverick-js/scheduler": "^1.0.0",
    "@microsoft/api-extractor": "^7.28.6",
    "@rollup/pluginutils": "^4.2.1",
    "@types/node": "^18.0.3",
    "benchmark": "^2.1.4",
    "esbuild": "^0.14.49",
    "export-size": "^0.5.2",
    "globby": "^13.1.2",
    "jsdom": "^20.0.0",
    "kleur": "^4.1.5",
    "node-fetch": "^3.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "standard-version": "^9.5.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.0",
    "vite": "^3.0.3",
    "vitest": "^0.19.0",
    "webpack": "^5.74.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx --no-install lint-staged",
    "commit-msg": "npx --no -- commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md,json}": "prettier --write"
  },
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maverick-js/elements.git"
  },
  "bugs": {
    "url": "https://github.com/maverick-js/elements/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "volta": {
    "node": "16.15.1"
  },
  "packageManager": "pnpm@7.2.1"
}
