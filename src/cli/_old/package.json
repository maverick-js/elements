{
  "name": "@vidstack/eliza",
  "version": "1.6.4",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "bin": {
    "eliza": "cli.js"
  },
  "exports": {
    ".": "./dist/node/index.js",
    "./cli.js": "./cli.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "bundle": "node ../../.scripts/bundle.js",
    "clean": "rimraf dist",
    "lint:eslint": "eslint src --fix",
    "lint:prettier": "prettier src --write --loglevel warn",
    "types": "node ../../.scripts/types.js",
    "watch": "run-p watch:*",
    "watch:bundle": "pnpm run bundle -w",
    "watch:types": "pnpm run types -w"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.20.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.17",
    "@types/normalize-path": "^3.0.0",
    "@types/yargs": "^17.0.8",
    "@vidstack/foundation": "workspace:*",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "fs-extra": "^10.0.0",
    "globby": "^13.1.1",
    "kleur": "^4.1.4",
    "lru-cache": "^7.4.4",
    "normalize-path": "^3.0.0",
    "read-pkg-up": "^9.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  }
}
